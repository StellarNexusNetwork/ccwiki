{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cc wiki project",
  "version": "0.0.1",
  "identifier": "com.cc.wiki.project.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "cc wiki project",
        "width": 1250,
        "height": 800,
        "minWidth": 700,
        "minHeight": 550,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "publisher": "Stellar Nexus Network",
    "copyright": "Copyright © 2022-present Stellar Nexus Network. All rights reserved. Created with ❤️ by cat and Vesta.",
    "license": "AGPL-3.0 license"
  }
}